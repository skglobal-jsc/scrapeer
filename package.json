{
  "name": "@sk-global/scrapeer",
  "description": "A collection of utilities for scraping data from the web.",
  "version": "1.0.37",
  "source": "src/index.ts",
  "main": "dist/index.umd.js",
  "module": "dist/index.modern.module.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**.js*",
    "dist/**/*.ts"
  ],
  "engines": {
    "node": ">=14.16"
  },
  "scripts": {
    "dev": "ts-node demo.ts",
    "build": "microbundle --define PKG_VERSION=$npm_package_version",
    "watch": "microbundle watch --define PKG_VERSION=$npm_package_version",
    "test": "jest --coverage",
    "lint": "prettier src test -c",
    "format": "prettier src test --write",
    "prepack": "rm -rf dist && npm run build",
    "release": "npm run prepack && source .env && npx np"
  },
  "devDependencies": {
    "@types/jest": "^29.0.3",
    "axios": "^0.27.2",
    "cheerio": "1.0.0-rc.3",
    "jest": "^29.0.3",
    "microbundle": "^0.15.0",
    "prettier": "^2.6.2",
    "string-similarity": "^4.0.4",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.9.1"
  },
  "prettier": {
    "singleQuote": true
  },
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "directories": {
    "test": "test"
  },
  "author": "SK-Global"
}
